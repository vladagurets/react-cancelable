    "test": "yarn build && yarn test:lazy",
    "watch": "tsc -w"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "devDependencies": {
    "@types/react": "^18.0.1",
